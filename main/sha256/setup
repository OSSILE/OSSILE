export PWD=$(pwd)
system -kpieb "CRTSRCPF FILE(OSSILE/CRTFRMSTMF) RCDLEN(198)"
system -kpieb "CRTCMOD MODULE(OSSILE/SHA256) SRCSTMF('$PWD/sha256.c') REPLACE(*YES) OPTION(*EVENTF) DBGVIEW(*SOURCE)"
system -kpieb "CRTCMOD MODULE(OSSILE/TEST) SRCSTMF('$PWD/test.c') REPLACE(*YES) OPTION(*EVENTF) DBGVIEW(*SOURCE)"
exec system -kpieb "CRTPGM PGM(OSSILE/TEST) MODULE(OSSILE/TEST OSSILE/SHA256)"
