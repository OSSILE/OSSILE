#!/QOpenSys/usr/bin/sh
export PWD=$(pwd)
system -kpieb "CRTLIB LIB(FTPCLNT)"
system -kpieb "CRTLIB LIB(FTPCLTOSS)"
system -kpieb "ADDLIBLE LIB(FTPCLTOSS)"
system -kpieb "CRTSRCPF FILE(FTPCLTOSS/QCLSRC) RCDLEN(150)"
system -kpieb "CRTSRCPF FILE(FTPCLTOSS/QCMDSRC) RCDLEN(150)"
system -kpieb "ADDPFM FILE(FTPCLTOSS/QCLSRC) MBR(CRTOBJS) SRCTYPE(CLLE)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/QCLSRC/CRTOBJS.CLLE') TOMBR('/QSYS.LIB/FTPCLTOSS.LIB/QCLSRC.FILE/CRTOBJS.MBR') MBROPT(*REPLACE)"
system -kpieb "ADDPFM FILE(FTPCLTOSS/QCMDSRC) MBR(CRTOBJS) SRCTYPE(CMD)"
system -kpieb "CPYFRMSTMF FROMSTMF('$PWD/QCMDSRC/CRTOBJS.CMD') TOMBR('/QSYS.LIB/FTPCLTOSS.LIB/QCMDSRC.FILE/CRTOBJS.MBR') MBROPT(*REPLACE)"
system -kpieb "CRTCLPGM PGM(FTPCLNT/CRTOBJS) SRCFILE(FTPCLTOSS/QCLSRC)"
system -kpieb "CRTCMD CMD(FTPCLNT/CRTOBJS) PGM(*LIBL/CRTOBJS) SRCFILE(FTPCLTOSS/QCMDSRC)"
system -kpieb "RMVLIBLE	LIB(FTPCLTOSS)"
system -kpieb "ADDLIBLE LIB(FTPCLNT)"
exec system -kpieb "CRTOBJS TGTLIB(FTPCLNT) SRCLIB(FTPCLTOSS) RLSLVL(*CURRENT) DIR('$PWD/'))"
system "RMVLIBLE LIB(FTPCLNT)"