#!/QOpenSys/usr/bin/sh
export PWD=$(pwd)
system "CRTLIB LIB(MD5CRC)"
system "CRTLIB LIB(MD5OSS)"
system "ADDLIBLE LIB(MD5OSS)"
system "CRTSRCPF FILE(MD5OSS/QCLSRC) RCDLEN(150)"
system "CRTSRCPF FILE(MD5OSS/QCMDSRC) RCDLEN(150)"
system "ADDPFM FILE(MD5OSS/QCLSRC) MBR(CRTOBJS) SRCTYPE(CLLE)"
system "CPYFRMSTMF FROMSTMF('$PWD/QCLSRC/CRTOBJS.CLLE') TOMBR('/QSYS.LIB/MD5OSS.LIB/QCLSRC.FILE/CRTOBJS.MBR') MBROPT(*REPLACE)"
system "ADDPFM FILE(MD5OSS/QCMDSRC) MBR(CRTOBJS) SRCTYPE(CMD)"
system "CPYFRMSTMF FROMSTMF('$PWD/QCMDSRC/CRTOBJS.CMD') TOMBR('/QSYS.LIB/MD5OSS.LIB/QCMDSRC.FILE/CRTOBJS.MBR') MBROPT(*REPLACE)"
system "CRTCLPGM PGM(&INTOLIB/CRTOBJS) SRCFILE(MD5OSS/QCLSRC)"
system "CRTCMD CMD(MD5CRC/CRTOBJS) PGM(*LIBL/CRTOBJS) SRCFILE(MD5OSS/QCMDSRC)"
system "RMVLIBLE LIB(MD5OSS)"
system "ADDLIBLE LIB(MD5CRC)"
exec system "CRTOBJS TGTLIB(MD5CRC) SRCLIB(MD5OSS) RLSLVL(*CURRENT) DIR('$PWD/')"
system "RMVLIBLE LIB(MD5CRC)"